{
  "plugins": ["@babel/plugin-proposal-class-properties"],
  "presets": [
    "@babel/preset-flow",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "modules": "auto",
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "env": {
    "commonjs": {
      "plugins": [["@babel/plugin-transform-modules-commonjs"]]
    }
  },
  "ignore": []
}
